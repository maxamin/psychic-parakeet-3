<?xml version="1.0" encoding="UTF-8"?>
<project xmlsn="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <groupId>jay-johnson</groupId>
    <artifactId>celery-connectors</artifactId>
    <version>6.0.0</version>
    <build>
        <plugins>
            <plugin>
                <groupId>org.owasp</groupId>
                <artifactId>dependency-check-maven</artifactId>
                <version>1.4.5</version>
                <configuration>
                    <jarAnalyzer>true</jarAnalyzer>
                    <pyDistributionAnalyzerEnabled>true</pyDistributionAnalyzerEnabled>
                    <pyPackageAnalyzerEnabled>true</pyPackageAnalyzerEnabled>
                    <rubygemsAnalyzerEnabled>true</rubygemsAnalyzerEnabled>
                    <opensslAnalyzerEnabled>true</opensslAnalyzerEnabled>
                    <cmakeAnalyzerEnabled>true</cmakeAnalyzerEnabled>
                    <autoconfAnalyzerEnabled>true</autoconfAnalyzerEnabled>
                    <composerAnalyzerEnabled>true</composerAnalyzerEnabled>
                    <nspAnalyzerEnabled>true</nspAnalyzerEnabled>
                    <swiftPackageManagerAnalyzerEnabled>true</swiftPackageManagerAnalyzerEnabled>
                    <assemblyAnalyzerEnabled>true</assemblyAnalyzerEnabled>
                    <cocoapodsAnalyzerEnabled>true</cocoapodsAnalyzerEnabled>
                    <nuspecAnalyzerEnabled>true</nuspecAnalyzerEnabled>
                    <failBuildOnCVSS>2</failBuildOnCVSS>
                    <mavenSettingsProxyId></mavenSettingsProxyId>
                    <cveValidForHours>0</cveValidForHours>
                </configuration>
                <executions>
                    <execution>
                        <goals>
                            <goal>check</goal>
                        </goals>
                    </execution>
                </executions>
            </plugin>
        </plugins>
    </build>
</project>
