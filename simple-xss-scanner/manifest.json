{
  "manifest_version": 2,

  "name": "XSS scanner",
  "description": "A slightly more reliable XSS scanner",
  "version": "0.1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Scan now!"
  },
  "permissions": [
    "activeTab"
  ],
  "web_accessible_resources": ["findVulns.js"]
}