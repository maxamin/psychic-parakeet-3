<ResourceDictionary x:Uid="ResourceDictionary_1" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

	<Style x:Uid="Style_1" x:Key="PasswordBoxStyle" TargetType="{x:Type PasswordBox}">
		<Setter x:Uid="Setter_1" Property="SnapsToDevicePixels" Value="true" />
		<Setter x:Uid="Setter_2" Property="OverridesDefaultStyle" Value="true" />
		<Setter x:Uid="Setter_3" Property="KeyboardNavigation.TabNavigation" Value="None" />
		<Setter x:Uid="Setter_4" Property="FocusVisualStyle" Value="{x:Null}" />
		<Setter x:Uid="Setter_5" Property="FontFamily" Value="Verdana" />
		<Setter x:Uid="Setter_6" Property="MinWidth" Value="120" />
		<Setter x:Uid="Setter_7" Property="MinHeight" Value="20" />
		<Setter x:Uid="Setter_8" Property="PasswordChar" Value="●"/>
		<Setter x:Uid="Setter_9" Property="AllowDrop" Value="true" />
		<Setter x:Uid="Setter_10" Property="Template" Value="{StaticResource PasswordBoxControlTemplate}"/>
		<Setter x:Uid="Setter_11" Property="Height" Value="30"/>
		<Setter x:Uid="Setter_12" Property="Padding" Value="5, 2, 5, 0"/>
		<Setter x:Uid="Setter_13" Property="Background" Value="White" />
		<Setter x:Uid="Setter_14" Property="BorderBrush" Value="#858585" />
		<Setter x:Uid="Setter_15" Property="BorderThickness" Value="3" />
		<Setter x:Uid="Setter_16" Property="Border.CornerRadius" Value="6" />

	</Style>

</ResourceDictionary>
	